<header>
   <div>
      <h1>Web Programming</h1>
      <div class="masthead">
         <p>
            {% for data in data %}
               {% for section in data.section %}
                  {% for content in section.content %}
                     <span>{{ content | safe }}</span>
                  {% endfor %}
               {% endfor %}
            {% endfor %}
         </p>
      </div>
   </div>
</header>
